<h1>Sign In</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

<%= form_for :user, url: login_path, method: "post" do |f| %>
  <div>
    <%= f.text_field :email, placeholder: "Email" %>
  </div>
  <br>
  <div>
    <%= f.password_field :password, placeholder: "Password" %>
  </div>
  <br>
  <%= f.submit %>

<% end %>